{
  "name": "model-api",
  "version": "1.0.0",
  "description": "Model project for api",
  "main": "index.js",
  "scripts": {
    "start": "node dist",
    "debug": "node --inspect dist",
    "build-dev": "webpack --config webpack/webpack.dev.js",
    "build-prod": "webpack --config webpack/webpack.prod.js",
    "watch": "nodemon --exec \"npm run build-dev && npm start\"",
    "watch-prod": "nodemon --exec \"npm run build-prod && npm start\"",
    "watch-debug": "nodemon --exec \"npm run build-dev && npm run debug\"",
    "linter": "eslint src",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "aws": "npm run build-prod && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benoitDD/model-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benoitDD/model-api/issues"
  },
  "homepage": "https://github.com/benoitDD/model-api#readme",
  "dependencies": {
    "core-js": "3.1.4",
    "express": "4.17.1",
    "loglevel": "1.6.3"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "dotenv": "8.0.0",
    "eslint": "5.16.0",
    "jest": "24.8.0",
    "nodemon": "1.19.1",
    "webpack": "4.34.0",
    "webpack-cli": "3.3.4",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2"
  }
}
